# -*- toolbox -*-
<doc type=BuildSystem::ToolDoc version=1.0>
<Tool name=SELF version=SELF>
<Client>
 <Environment name=LIBDIR value="$LOCALTOP/lib/$SCRAM_ARCH">
 </Environment>
 <Environment name=CMSSW_BINDIR value="$LOCALTOP/bin/$SCRAM_ARCH">
 </Environment>
 <Environment name=INCLUDE value="$LOCALTOP/src">
 </Environment>
</Client>
<Runtime name=CMSSW_BASE value="$LOCALTOP">
<Runtime name=CMSSW_RELEASE_BASE value="$RELEASETOP">
<Runtime name=LOCALRT value="$LOCALTOP">
<Runtime name=CMSSW_VERSION value="$SCRAM_PROJECTVERSION">
<Runtime name=LD_LIBRARY_PATH value="$LIBDIR" type=path>
<Runtime name=PATH value="$CMSSW_BINDIR" type=path>
<Runtime name=SEAL_PLUGINS value="$LOCALTOP/module/$SCRAM_ARCH" type=path>
<Runtime name=CMSSW_SEARCH_PATH value="$LOCALTOP/src" type=path handler=warn>
<Runtime name=CMSSW_SEARCH_PATH value="$LOCALTOP/share" type=path handler=warn>
<Flags CPPDEFINES="PROJECT_NAME=$(SCRAM_PROJECTNAME)">
<Flags CPPDEFINES="PROJECT_VERSION=$(SCRAM_PROJECTVERSION)">
</Tool>
